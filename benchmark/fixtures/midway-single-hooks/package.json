{
  "name": "midway-single-hooks",
  "version": "1.0.0",
  "description": "midway-sample",
  "private": true,
  "dependencies": {
    "midway": "1.20.3",
    "@midwayjs/cli": "1.2.93",
    "@midwayjs/cli-plugin-build": "1.2.93",
    "pandora": "1.10.6"
  },
  "devDependencies": {
    "@types/mocha": "5.2.7",
    "@types/node": "16.11.17",
    "ts-node": "10.4.0",
    "tslib": "2.3.1",
    "tslint": "5.20.1",
    "typescript": "4.5.4"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "start": "NODE_ENV=production pandora start .",
    "stop": "pandora exit",
    "start_build": "npm run build && NODE_ENV=production midway-bin dev",
    "pdev": "pandora dev",
    "dev": "NODE_ENV=local midway-bin dev --ts",
    "debug": "NODE_ENV=local midway-bin debug --ts",
    "test": "npm run lint -- --fix && midway-bin test --ts",
    "cov": "midway-bin cov --ts",
    "lint": "tslint -c tslint.json --project .",
    "ci": "midway-bin cov --ts",
    "build": "midway-bin build -c",
    "ready": "npm run build"
  },
  "ci": {
    "version": "8, 9"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "midway-server-options": {
    "workers": 1
  }
}
