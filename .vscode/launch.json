  "version": "0.2.0",
  "configurations": [
    {
      "name": "Miday Test",
      "type": "node",
      "autoAttachChildProcesses": true,
      "console": "integratedTerminal",
      "env": {
        "NODE_ENV": "local",
        "TS_NODE_PROJECT": "test/tsconfig.json",
        "PGHOST": "",
        "PGUSER": "",
        "PGPASSWORD": "",
      },
      "port": 9229,
      "protocol": "auto",
      "request": "launch",
      "restart": true,
      "runtimeArgs": [
        "run",
        "devtest",
        "--",
        "--inspect-brk"
      ],
      "runtimeExecutable": "npm",
      "skipFiles": [
        "${workspaceFolder}/../../node_modules/rxjs/**/*.js",
        "${workspaceFolder}/node_modules/rxjs/**/*.js",
        "<node_internals>/**/*.js"
      ],
      "timeout": 60000
    },
  ]
}
