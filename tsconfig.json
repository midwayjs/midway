{
  "extends": "./node_modules/mwts/tsconfig-midway.json",
  "compileOnSave": true,
  "compilerOptions": {
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noImplicitThis": false,
    "noUnusedLocals": true,
    "stripInternal": true,
    "noImplicitReturns": false,
    "strict": false,
    "noEmitOnError": false,
    "skipLibCheck": true
  },
  "exclude": [
    "dist",
    "node_modules",
    "test",
    "site"
  ],
}
