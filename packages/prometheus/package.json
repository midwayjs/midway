    "index.d.ts"
  ],
  "license": "MIT",
  "devDependencies": {
    "@midwayjs/core": "workspace:^",
    "@midwayjs/koa": "workspace:^",
    "@midwayjs/mock": "workspace:^",
    "@types/request": "2.48.12"
  },
  "dependencies": {
    "prom-client": "^14.0.0",
    "request": "2.88.2"
  },
  "engines": {
    "node": ">=12"
  }
}
