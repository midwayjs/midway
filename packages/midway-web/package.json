    "config",
    "dist",
    "d.ts"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^3.5.2",
    "@types/lodash": "^4.14.109",
    "@types/mocha": "^2.2.41",
    "@types/node": "^10.5.5",
    "egg-mock": "^3.17.3",
    "midway-bin": "^0.3.8",
    "pedding": "^1.1.0"
  },
  "dependencies": {
    "camelcase": "^5.0.0",
    "debug": "^3.1.0",
    "egg": "^2.9.1",
    "extend": "^3.0.0",
    "globby": "^8.0.1",
    "inflection": "^1.12.0",
    "injection": "^0.4.5",
    "is-type-of": "^1.2.0",
    "midway-core": "^0.4.5"
  },
