{
  "name": "midway-web",
  "version": "0.4.5",
  "description": "Midway Web Scene",
  "main": "dist/index",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "npm run lint && midway-bin build -c",
    "lint": "../../node_modules/.bin/tslint --format prose -c ../../tslint.json src/**/*.ts test/**/*.ts",
