{
  "name": "{{name}}",
  "version": "1.0.0",
  "description": "{{description}}",
  "private": true,
  "dependencies": {
    "midway": "^1.0.0",
    "egg-scripts": "^2.10.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.5.5",
    "egg-ci": "^1.8.0",
    "midway-bin": "1",
    "midway-mock": "1",
    "tslib": "^1.8.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.9.1",
    "typescript": "^3.2.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-{{name}} --framework=midway",
    "stop": "egg-scripts stop --title=egg-server-{{name}}",
    "start_build": "npm run build && NODE_ENV=development midway-bin dev",
    "dev": "NODE_ENV=local midway-bin dev --ts",
    "debug": "NODE_ENV=local midway-bin debug --ts",
    "test": "npm run lint && midway-bin test --ts",
    "cov": "midway-bin cov --ts",
    "lint": "tslint -c tslint.json --project .",
    "ci": "npm run cov",
    "build": "midway-bin build -c"
  },
  "ci": {
    "version": "8, 10"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "{{author}}",
  "license": "MIT"
}
