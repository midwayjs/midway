{
  "name": "@midwayjs/core",
  "version": "2.1.1",
  "description": "midway core",
  "main": "dist/index",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "npm run lint && midway-bin build -c",
    "lint": "../../node_modules/.bin/tslint --format prose -c ../../tslint.json --fix 'src/**/*.ts' 'test/**/*.ts'",
    "test": "npm run lint && midway-bin clean && NODE_ENV=test midway-bin test --ts",
    "cov": "midway-bin clean && midway-bin cov --ts",
    "autod": "midway-bin autod",
    "link": "npm link"
  },
  "keywords": [
    "midway",
    "IoC",
    "core"
  ],
  "files": [
    "src",
    "dist",
    "d.ts"
  ],
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.2.0",
    "midway-bin": "^2.0.15",
    "mm": "^2.5.0",
    "sinon": "^7.2.2"
  },
  "dependencies": {
    "@midwayjs/decorator": "^2.1.1",
    "debug": "^4.1.1",
    "extend2": "^1.0.0",
    "globby": "^10.0.1",
    "is-type-of": "^1.2.1",
    "lodash.assign": "^4.2.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.defaultsdeep": "^4.6.0",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "lodash.template": "^4.4.0",
    "reflect-metadata": "^0.1.13"
  },
  "author": "Harry Chen <czy88840616@gmail.com>",
  "repository": {
    "type": "git",
    "url": "http://github.com/midwayjs/midway.git"
  }
}
