    "@midwayjs/bootstrap": "2.14.0",
    "@midwayjs/core": "2.14.0",
    "@midwayjs/decorator": "2.14.0",
    "@midwayjs/socketio": "2.14.0",
    "@midwayjs/web": "2.14.2",
    "egg": "^2.0.0"
  },
  "devDependencies": {
    "@midwayjs/cli": "1.2.93",
    "@midwayjs/egg-ts-helper": "1.2.1",
    "@midwayjs/luckyeye": "1.0.3",
    "@midwayjs/mock": "2.14.0",
    "@types/jest": "^26.0.10",
    "@types/node": "14.18.5",
    "@types/socket.io-client": "1.4.36",
    "cross-env": "6.0.3",
    "jest": "26.6.3",
